{
  "name": "require-compiled",
  "version": "1.4.0",
  "description": "The easiest way to require babel-compiled modules.",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:unit": "tape test/*.js | faucet",
    "test:lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwoudenberg/require-compiled.git"
  },
  "keywords": [
    "babel",
    "es6",
    "es7",
    "es2015"
  ],
  "author": "Jasper Woudenberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwoudenberg/require-compiled/issues"
  },
  "homepage": "https://github.com/jwoudenberg/require-compiled#readme",
  "standard": {
    "ignore": [
      "/test/fixtures/"
    ],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "babel-core": "6.11.4",
    "babel-plugin-detective": "2.0.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "callsite": "1.0.0",
    "find-cache-dir": "0.1.1",
    "md5-hex": "1.3.0",
    "resolve": "1.1.7",
    "source-map-support": "0.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.3",
    "babel-preset-es2015": "6.13.0",
    "faucet": "0.0.1",
    "npm-run-all": "2.3.0",
    "standard": "7.1.2",
    "tape": "4.6.0",
    "xyz": "^0.5.0"
  }
}
